{
  "name": "documantis-parent",
  "private": true,
  "version": "0.1.0",
  "description": "DocuMantis - Financial Advisor PDF Automation Tool",
  "scripts": {
    "dev": "./start-dev-servers.sh",
    "dev:backend": "cd app && python -m uvicorn main:app --reload --port 8001",
    "dev:frontend": "cd frontend && npm run dev:frontend",
    "setup": "pip install -r requirements.txt && cd frontend && npm install",
    "install:backend": "pip install -r requirements.txt",
    "install:frontend": "cd frontend && npm install"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
} 